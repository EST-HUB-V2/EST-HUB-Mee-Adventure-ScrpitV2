Tab:AddToggle({
    Name = "Fishing Snow",
    Default = false,
    Callback = function(Snow)
        _G.ez = Snow
while _G.ez do wait()
  local args = {
    [1] = "Fishing1"
}

game:GetService("Players").LocalPlayer.Character.Fishing.Snow.RemoteEvent:FireServer(unpack(args))
end
    end
})

