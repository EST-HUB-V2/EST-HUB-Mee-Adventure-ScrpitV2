Tab:AddDropdown({
	Name = "Select Mob | Sea",
	Default = "Crying Emoji",
	Options = {"Crying Emoji", "Nerd Emoji", },
	Callback = function(MobSea)
    getgenv().AutoFarmSea = MobSea
